{
  "name": "ideatocode/nova-tooltip-field",
  "description": "A Laravel Nova field to show additional data but not clutter the table.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0",
    "laravel/nova": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Ideatocode\\NovaTooltipField\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ideatocode\\NovaTooltipField\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}